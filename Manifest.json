{
    "manifest_version": 3,
    "name": "Random Quote Generator",
    "version": "1.0",
    "description": "Displays a random quote every time you click the extension icon.",
    "icons": {
        "16": "joke.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "joke.png"
      }
    },
    "permissions": ["activeTab"]
  }  